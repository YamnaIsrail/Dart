// FOR LOOP is definite loop (if you know how many times to iterate)
// and WHILE LOOP and DO WHILE LOOP is indefinite  (if you don't know how many times to iterate)

void main()
{
  for(int i=1; i<5; i++)
    {
      print("For loop");
    }

  int i=1;
  while(i<5)
    {
      print("While loop");
      i++;
    }

    do{
      print("Do while");
    } while (i<5);
}
